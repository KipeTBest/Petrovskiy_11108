package Sort;

public class CalculateNumbers {

    // считает количество отрицательных чисел
    public static int calc(int[] array) {
        int counter = 0;
        for (int i = 0; i < array.length; i++) {
            if (array[i] < 0) {
                counter++;
            }
        }
        return counter;
    }

}
